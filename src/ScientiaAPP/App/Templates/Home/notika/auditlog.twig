<style>
.json-table {
    border-collapse: collapse;
    border-spacing: 5;
    font-family: monospace, monospace;
    font-size: 1em;
}
</style>
<!-- Breadcomb area Start-->
<div class="breadcomb-area">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="breadcomb-list">
					<div class="row">
						<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
							<div class="breadcomb-wp">
								<div class="breadcomb-icon">
									<i class="notika-icon notika-settings animated infinite flip"></i>
								</div>
								<div class="breadcomb-ctn">
									<h2>Auditlog</h2>
									<p>User log activity</p>
								</div>
							</div>
						</div>
						<div class="col-lg-6 col-md-6 col-sm-6 col-xs-3">
							<div class="breadcomb-report">
								<button id="tikClock" data-toggle="tooltip" data-placement="left" title="" class="btn">
									<i class="fa fa-clock-o"></i>
								</button>
							</div>
						</div>
					</div>
				</div>

                <div class="form-element-list">
                    <div class="row margin-bt-20">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                            <div class="nk-int-mk sl-dp-mn">
                                <h2>Periode</h2>
                            </div>
                            <div id="reportrange" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc; width: 100%">
                                <i class="fa fa-calendar"></i>&nbsp;
                                <span></span> <i class="fa fa-caret-down"></i>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                            <div class="nk-int-mk sl-dp-mn">
                                <h2>Method</h2>
                            </div>
                            <span style="margin-left: 10px;"><i class="fa fa-spinner fa-spin"></i> Please wait...</span>
                            <div class="chosen-select-act fm-cmp-mg" style="display:none;">
                                <select id="http_method" name="http_method" class="chosen http_method">
                                    <option value="">SEMUA</option>
                                    <option value="GET">GET</option>
                                    <option value="POST">POST</option>
                                    <option value="PUT">PUT</option>
                                    <option value="DELETE">DELETE</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:10px;">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="btn-demo-notika" style="padding:0;">
                                <div class="btn-list">
                                    <button type="button" id="btApply"
                                        class="btn btn-success notika-btn-success waves-effect waves-light"
                                        data-loading-text="Loading..." title="Terapkan Filter">
                                        <i class="notika-icon notika-checked" style="font-weight:bolder;"></i>
                                        <span style="font-weight:bold; margin-left: 5px;">Terapkan</span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
			</div>
		</div>
	</div>
</div>
<!-- Breadcomb area End-->
<!-- Main area Start-->
<div class="colr-area">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="color-wrap">
					<div class="color-hd">
						<div class="x_content">
							<div class="cssload-loader">Please Wait</div>
							<div id="dtableDiv" style="display:none;">
								<table id="datatable-responsive"
									class="table table-striped table-bordered table-hover dt-responsive wrap"
									cellspacing="0" width="100%">
									<thead>
										<tr>
											<th width="1%"></th>
											<th width="2%" class="text-center">NO</th>
											<th width="10%">USERNAME</th>
											<th width="22%">FULLNAME</th>
											<th width="15%">DATETIME</th>
											<th width="25%">URL</th>
											<th width="10%">METHOD</th>
											<th width="15%">IP ADDRESS</th>
											<th width="5%">DATA</th>
										</tr>
									</thead>
								</table>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- Main area End-->
<!-- #region modal -->
<div class="modal fade formEditorModal" data-backdrop="static" role="dialog" aria-hidden="true">
	<div class="modal-dialog modals-default">
		<div class="modal-content" style="padding: 30px; 30px;">
			<div class="modal-header form-group-15">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title">Preview Data</h4>
			</div>
			<div class="modal-body">
                <table class="json-table" width="100%" style="margin-bottom:15px;">
                    <tr>
                        <td width="12%">Time</td>
                        <td width="3%">:</td>
                        <td><span id="js_time"></span></td>
                    </tr>
                    <tr>
                        <td>Username</td>
                        <td>:</td>
                        <td><span id="js_username"></span></td>
                    </tr>
                    <tr>
                        <td>URL</td>
                        <td>:</td>
                        <td>/<span id="js_url"></span></td>
                    </tr>
                    <tr>
                        <td>Method</td>
                        <td>:</td>
                        <td><span id="js_method"></span></td>
                    </tr>
                    <tr>
                        <td>From</td>
                        <td>:</td>
                        <td><span id="js_from"></span></td>
                    </tr>
                </table>
                <pre id="json-renderer"></pre>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn_cancel btn btn-danger notika-btn-danger waves-effect" data-dismiss="modal">
					<i class="fa fa-times"></i> Close
				</button>
			</div>
		</div>
	</div>
</div>
<!-- #endregion modal -->