<!-- Main area Start-->
<div class="colr-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="color-wrap">
                    <div class="color-hd">
                        <div class="x_content">
                            <div class="cssload-loader">Please Wait</div>
                            <div id="dtableDiv" style="display:none;">
                                <div class="row margin-bt-20">
                                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                                        <div class="nk-int-mk sl-dp-mn">
