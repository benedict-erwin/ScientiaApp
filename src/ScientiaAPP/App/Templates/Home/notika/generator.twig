</div>
<!-- Main area End-->
<!-- #region modal -->
<div class="modal fade formEditorModal" data-backdrop="static" role="dialog" aria-hidden="true">
    <div class="modal-dialog modals-default">
        <div class="modal-content">
            <div class="modal-header form-group-15">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Generate New Module (CRUD)</h4>
            </div>
            <div class="modal-body">
                <form id="formEditor" data-parsley-validate class="form-horizontal form-label-left">
                    <div class="form-group-15">
                        <label>Group Menu <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <span style="margin-left: 10px;"><i class="fa fa-spinner fa-spin"></i> Please wait...</span>
                            <div class="chosen-select-act fm-cmp-mg" style="display:none;">
                                <select id="m_groupmenu" name="m_groupmenu" class="chosen m_groupmenu"
                                    required="required" data-parsley-group="role"></select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Nama Menu <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <input type="hidden" name="action" value="generate">
                            <input type="text" class="form-control input-sm" type="text" name="menu" required="required"
                                data-parsley-group="role" placeholder="Nama Menu">
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Table Source <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <span style="margin-left: 10px;"><i class="fa fa-spinner fa-spin"></i> Please wait...</span>
                            <div class="chosen-select-act fm-cmp-mg" style="display:none;">
                                <select id="get_tables" name="get_tables"
                                    class="get_tables form-control input-sm chosen" required="required"
                                    data-parsley-group="role"></select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>URL <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <input type="text" name="url" required="required" class="form-control input-sm"
                                data-parsley-group="role" placeholder="URL">
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Controller <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <input type="text" name="controller" required="required" class="form-control input-sm"
                                data-parsley-group="role" placeholder="Controller">
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Generate <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <div class="form-check" style="margin-top: 7px;">
                                <input class="form-check-input" type="radio" name="generate" id="genBO" value="BO"
                                    checked>
                                <label class="form-check-label" for="genBO">
                                    BackEnd Only
                                </label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="radio" name="generate" id="genBF" value="BF">
                                <label class="form-check-label" for="genBF">
                                    BackEnd and FrontEnd
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Action <span class="required">*</span></label>
                        <div class="nk-int-st">
                            <div class="form-check">
                                <input id="create" class="form-check-input" type="checkbox" name="crud[]" value="c"
                                    data-parsley-mincheck="1" data-parsley-group="role" />
                                <label class="form-check-label" for="create">Create</label>
                                <input id="read" class="form-check-input" type="checkbox" name="crud[]" value="r"
                                    style="margin-left: 15px;" />
                                <label class="form-check-label" for="read">Read</label>
                                <input id="update" class="form-check-input" type="checkbox" name="crud[]" value="u"
                                    style="margin-left: 15px;" />
                                <label class="form-check-label" for="update">Update</label>
                                <input id="delete" class="form-check-input" type="checkbox" name="crud[]" value="d"
                                    style="margin-left: 15px;" />
                                <label class="form-check-label" for="delete">Delete</label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group-15">
                        <label>Role <span class="required">*</span></label>
                        <div id="cb_jabatan" class="nk-int-st"></div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn_save btn btn-default notika-btn-default waves-effect"
                    data-loading-text="Loading...">
                    <i class="fa fa-save"></i> Save
                </button>
                <button type="button" class="btn_cancel btn btn-danger notika-btn-danger waves-effect"
                    data-dismiss="modal">
                    <i class="fa fa-times"></i> Close
                </button>
            </div>
        </div>
    </div>
</div>
<!-- #endregion modal -->